name: figure figure*
<div class="figure" id="{{ obj.title.id}}">
  {{ obj }}
</div>

name: table table*
<div class="table"  id="{{ obj.title.id}}">
  {{ obj }}
</div>

name: marginpar
<p class="marginpar">{{ obj.attributes.right }}</p>

name: caption
<div class="caption">
  <span class="caption_title">{{ obj.title }}</span> 
  <span class="caption_ref">{{ obj.ref }}</span> 
  <span class="caption_text">{{ obj }}</span> 
</div>
